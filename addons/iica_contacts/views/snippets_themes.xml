<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="mass_mailing.email_designer_snippets" inherit_id="web_editor.snippets" primary="True">
      <xpath expr="//h2[@id='snippets_menu']" position="replace">
          <h2 id="snippets_menu">Select a template</h2>
      </xpath>
      <xpath expr="//div[@id='o_scroll']" position="replace">
          <div id="o_scroll">
              <div id="email_designer_themes">
                  <div data-name="iica_agro_enlace" groups="iica_contacts.iica_mailing_agro_enlace"
                       data-img="/iica_contacts/static/src/img/thumbnails/agro_enlace_thumb"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_agro_enlace"/>
                  </div>
                  <div data-name="iica_agro_enlace_live" groups="iica_contacts.iica_mailing_agro_enlace"
                       data-img="/iica_contacts/static/src/img/thumbnails/agro_enlace_live_thumb"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_agro_enlace_live"/>
                  </div>
                  <div data-name="iica_conexion" groups="iica_contacts.iica_mailing_conexion"
                       data-img="/iica_contacts/static/src/img/thumbnails/iica_conexion_thumb"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_conexion"/>
                  </div>
                  <div data-name="iica_connection" groups="iica_contacts.iica_mailing_conexion"
                       data-img="/iica_contacts/static/src/img/thumbnails/iica_connection_thumb"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_connection"/>
                  </div>                  
                  <div data-name="iica_prensa" groups="iica_contacts.iica_mailing_press"
                       data-img="/iica_contacts/static/src/img/thumbnails/iica_press_thumb"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_prensa"/>
                  </div>
                  <div data-name="iica_good_news" groups="iica_contacts.iica_mailing_journalist"
                       data-img="/iica_contacts/static/src/img/thumbnails/good_news_thumb"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_good_news"/>
                  </div>
                  <div data-name="iica_buenas_noticias" groups="iica_contacts.iica_mailing_journalist"
                       data-img="/iica_contacts/static/src/img/thumbnails/buenas_noticias_thumb"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_buenas_noticias"/>
                  </div>
                  <div data-name="iica_comunicado_prensa_es" groups="iica_contacts.iica_mailing_journalist"
                       data-img="/iica_contacts/static/src/img/thumbnails/press_release_spanish"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_comunicado_prensa_es"/>
                  </div>  
                  <div data-name="iica_comunicado_prensa_en" groups="iica_contacts.iica_mailing_journalist"
                       data-img="/iica_contacts/static/src/img/thumbnails/press_release_english"
                       data-images-info='{"logo": {"format": "png"}}'>
                      <t t-call="iica_contacts.theme_iica_comunicado_prensa_en"/>
                  </div>                  
              </div>
              <div id="iica_designer_conexion_agroenlace" class="o_panel" groups="iica_contacts.iica_mailing_conexion,iica_contacts.iica_mailing_agro_enlace">
                <div class="o_panel_header">
                    <i class="fa fa-envelope-open"/> IICA Conexión / Agroenlace
                </div>
                <div class="o_panel_body">
                <t t-snippet="iica_contacts.s_iica_block_image_text" t-thumbnail="/mass_mailing/static/src/img/blocks/block_image_text.png"/>
                <t t-snippet="iica_contacts.s_iica_block_image" t-thumbnail="/mass_mailing/static/src/img/blocks/block_image.png"/>
                <t t-snippet="iica_contacts.s_iica_block_three_cols" t-thumbnail="/mass_mailing/static/src/img/blocks/block_three_cols.png"/>
                <t t-snippet="iica_contacts.s_iica_only_text" t-thumbnail="/mass_mailing/static/src/img/blocks/block_paragraph.png"/>
                </div>
              </div>
              <div id="iica_designer_conexion_agroenlace" class="o_panel" groups="iica_contacts.iica_mailing_journalist">
                <div class="o_panel_header">
                    <i class="fa fa-envelope-open"/> Buenas noticias / Good news
                </div>
                <div class="o_panel_body">
                <t t-snippet="iica_contacts.s_iica_good_news_top" t-thumbnail="/mass_mailing/static/src/img/blocks/block_image_text.png"/>
                <t t-snippet="iica_contacts.s_iica_good_news_image_right" t-thumbnail="/mass_mailing/static/src/img/blocks/block_image.png"/>
                <t t-snippet="iica_contacts.s_iica_good_news_image_right" t-thumbnail="/mass_mailing/static/src/img/blocks/block_image.png"/>
                </div>
              </div>
              <div id="iica_designer_press" class="o_panel" groups="iica_contacts.iica_mailing_press">
                  <div class="o_panel_header">
                      <i class="fa fa-bullhorn"/> Press Newsletter
                  </div>
                  <div class="o_panel_body">
                  <t t-snippet="iica_contacts.s_iica_prensa_r_norte" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_sub.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_norte" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_text.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_r_central" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_sub.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_central" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_text.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_r_caribe" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_sub.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_caribe" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_text.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_r_andina" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_sub.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_andina" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_text.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_r_sur" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_sub.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_sur" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_text.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_r_otros" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_sub.png"/>
                    <t t-snippet="iica_contacts.s_iica_prensa_otros" t-thumbnail="/mass_mailing/static/src/img/blocks/block_title_text.png"/>
                    <t t-snippet="mass_mailing.s_mail_block_footer_separator" t-thumbnail="/mass_mailing/static/src/img/blocks/block_footer_separator.png"/>
                  </div>
              </div>
          </div>
      </xpath>

      <xpath expr="//div[@id='snippet_options']" position="inside">
          <t t-call="mass_mailing.snippet_options"/>
      </xpath>
  </template>

  <!--Headers y logos IICA-->
  <template id="s_iica_aniversary_logo" name="Aniversary IICA logo">
    <div class="o_mail_block_header_social">
